{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "sourceMap": true
  },
  "include": [
    "src/**/*",
   "worker-configuration.d.ts"
, "../env.d.ts"  ],
  "exclude": ["node_modules", "dist"]
}